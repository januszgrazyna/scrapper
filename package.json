{
  "name": "scrapper",
  "version": "0.0.1",
  "description": "scrapper",
  "main": "dist/index.js",
  "scripts": {
    "test": "npx jest",
    "start-impldownload": "cd buildSrc/dist && node buildSrc/index.js",
    "build-impldownload-local": "tsc --build ./buildSrc/tsconfig.json && cp -r ./src/config ./buildSrc/dist/src/config",
    "build-impldownload": "tsc --build ./buildSrc/tsconfig.json",
    "clean-impldownload": "rm -rf ./buildSrc/dist",
    "build-dev": "tsc --build tsconfig.json && copyfiles run.txt ./dist && tsc-alias -p tsconfig.json && copyfiles impls/**/*.pug dist/ && copyfiles impls/**/*.json dist/",
    "build": "tsc && copyfiles package.json run.txt ./dist && (cd dist && yarn install --production --force) && copyfiles impls/**/*.pug dist/ && copyfiles impls/**/*.json dist/",
    "start": "cd dist && node src/index.js",
    "start-dev": "cd dist && node src/index.js",
    "start-file": "bash start.sh",
    "clean": "rm -rf ./dist",
    "clean-outputs": "rm -rf ./outputs"
  },
  "dependencies": {
    "bezier-js": "4.1.0",
    "fast-crc32c": "^2.0.0",
    "firebase": "^9.0.2",
    "firebase-admin": "^9.11.1",
    "nodemailer": "^6.6.5",
    "pug": "^3.0.2",
    "puppeteer": "^10.1.0",
    "puppeteer-extra": "^3.1.18",
    "puppeteer-extra-plugin-stealth": "^2.7.8",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "xmlhttprequest": "^1.8.0",
    "yargs": "^16.2.0",
    "crypto-js": "^4.1.1"
  },
  "devDependencies": {
    "@types/pug": "^2.0.5",
    "@types/bezier-js": "4.1.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^16.0.0",
    "@types/puppeteer": "^5.4.4",
    "@types/uuid": "^8.3.0",
    "@types/winston": "^2.4.4",
    "@types/yargs": "^15.0.11",
    "@types/nodemailer": "^6.4.4",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "@types/crypto-js": "^4.0.2",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tsc-alias": "^1.3.9",
    "typescript": "^4.1.3",
    "copyfiles": "^2.4.1"
  },
  "author": "",
  "license": "ISC"
}
